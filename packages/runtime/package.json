{
    "name": "@zenstackhq/runtime",
    "version": "3.0.0-alpha.1",
    "description": "ZenStack Runtime",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "build": "tsup-node",
        "watch": "tsup-node --watch",
        "test": "vitest"
    },
    "keywords": [],
    "author": "ZenStack Team",
    "license": "MIT",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
